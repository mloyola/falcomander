{% extends '::frontend.html.twig' %}

{% block carousel %}
    <div id="myCarousel" class="carousel slide">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <div class="fill" style="background-image:url('http://localhost/falcomander/web/bundles/frontend/images/slide/01.png');"></div>
                <div class="carousel-caption">
                    <h1>{{ "Somos lideres en Control de Plaga Aviar" | trans }}</h1>
                </div>
            </div>
            <div class="item">
                <div class="fill" style="background-image:url('http://localhost/falcomander/web/bundles/frontend/images/slide/02.png');"></div>
                <div class="carousel-caption">
                    <h1>{{ "Brindamos calidad en la atención y servicio" | trans }}</h1>
                </div>
            </div>
            <div class="item">
                <div class="fill" style="background-image:url('http://localhost/falcomander/web/bundles/frontend/images/slide/03.png');"></div>
                <div class="carousel-caption">
                    <h1>{{ "Comprometidos con el medio ambiente" | trans }}</a>
                    </h1>
                </div>
            </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="icon-prev"></span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="icon-next"></span>
        </a>
    </div>
{% endblock %}

